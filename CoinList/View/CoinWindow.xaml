<Window x:Class="CoinList.View.CoinWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoinList.View"
        mc:Ignorable="d"
        Title="{Binding Name}" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="161,194,0,0" TextWrapping="Wrap" Text="{Binding Name}" VerticalAlignment="Top" Width="110"/>

    </Grid>
</Window>
